    "@backbase/messages-client-inbox-journey-ang": "0.8.1",
    "@backbase/positive-pay-journey-ang": "0.35.0",
    "@backbase/stop-checks-journey-ang": "1.2.1",
    "@backbase/trade-finance-journey-ang": "1.0.0-beta.6",
    "@backbase/ui-ang": "~6.34.2",
    "@backbase/universal-ang": "~7.2.1",
    "@ng-bootstrap/ng-bootstrap": "~9.0.0",
    "@ng-select/ng-select": "~6.0.0",
