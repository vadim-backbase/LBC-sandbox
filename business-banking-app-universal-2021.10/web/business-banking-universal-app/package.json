{
  "name": "business-banking-universal-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "e2e": "ng e2e",
    "format": "prettier --write \"{apps,libs}/**/*.ts\"",
    "format:check": "prettier --list-different \"{apps,libs}/**/*.ts\"",
    "lint": "ng lint",
    "ng": "ng",
    "package:apps": "bb-ang build-apps --output-hashing --serial-build --build-memory=8192",
    "postpackage:apps": "bb-ang create-provisioning-package",
    "package:libs": "bb-ang package-libs",
    "prestart": "bb-ang copy-assets",
    "start": "cross-env NODE_OPTIONS=--max_old_space_size=4096 ng serve --port 4300",
    "test": "ng test",
    "update": "ng update @bb-cli/schematics@^5 @backbase/foundation-ang@^6 @backbase/banking-schematics@^1",
    "xi18n:business-banking-app": "cross-env NODE_OPTIONS=--max_old_space_size=4096 ng extract-i18n business-banking-app --output-path=apps/business-banking-app/src/locale && xliffmerge --profile apps/business-banking-app/src/xliffmerge.json en-GB",
    "start:en-GB": "npm run start -- --configuration=en-GB",
    "create-experience:business-banking-app": "bb model json-model-to-cx-provisioning-package apps/business-banking-app/sdlc/experience.json",
    "bundle-home-page:business-banking-app": "bb model json-model-to-simplified-model apps/business-banking-app/sdlc/experience.json apps/business-banking-app/sdlc/pages/home.json -l silent --remove-defaults false -o dist/apps/business-banking-app/page-model-mock.json",
    "xi18n:business": "npm run xi18n:business-banking-app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.0",
    "@angular/cdk": "~11.2.0",
    "@angular/common": "~11.2.0",
    "@angular/compiler": "~11.2.0",
    "@angular/core": "~11.2.0",
    "@angular/forms": "~11.2.0",
    "@angular/localize": "~11.2.0",
    "@angular/platform-browser": "~11.2.0",
    "@angular/platform-browser-dynamic": "~11.2.0",
    "@angular/router": "~11.2.0",
    "@backbase/account-statement-business-journey-ang": "1.0.0",
    "@backbase/accounts-journey-ang": "0.16.0",
    "@backbase/ach-positive-pay-journey-ang": "0.16.0",
    "@backbase/actions-business-notification-preferences-journey-ang": "1.0.2",
    "@backbase/banking-schematics": "^1.0.0-beta.373",
    "@backbase/batch-journey-ang": "0.9.0",
    "@backbase/batch-templates-journey-ang": "0.3.0",
    "@backbase/business-ang": "3.10.3",
    "@backbase/cash-flow-journey-ang": "1.1.2",
    "@backbase/contact-journey-ang": "0.2.0",
    "@backbase/data-ang": "3.10.3",
    "@backbase/entitlements-ang": "3.10.3",
    "@backbase/flow-interaction-sdk-ang": "^2.2.0",
    "@backbase/foundation-ang": "~6.13.3",
